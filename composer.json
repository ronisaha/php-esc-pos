{
    "name": "xiidea/php-esc-pos",
    "type": "library",
    "homepage": "https://github.com/ronisaha/php-esc-pos/",
    "description": "EPSON ESC/POS printer command generator library",
    "keywords": ["ESC-POS", "Thermal printer", "TM-T70", "Epson"],
    "license": "MIT",
    "authors": [
        {
            "name": "Roni Saha",
            "email": "roni.cse@gmail.com"
        },
        {
            "name": "Michael Billington",
            "email": "michael.billington@gmail.com"
        },
        {
            "name": "Gergely Radics",
            "email": "gerifield@ustream.tv"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "suggest": {
        "hyperthese/php-serial": "Allows Communicate with devices connected over Serial port",
        "intervention/image": "Require when you want to print image",
        "endroid/qrcode": "Require when you want to print QRCode"
    },
    "autoload": {
        "psr-0": {
            "Epson\\": ["src"]
        }
    },
    "branch-alias": {
        "dev-master": "1.0.x-dev"
    }
}
